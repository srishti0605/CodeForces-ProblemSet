#include<iostream>
#include<conio.h>
#include<string.h>
#include <bits/stdc++.h>
using namespace std;
int main()
{   
    string s;
    cin>>s;
    for(int i=0;i<s.length();i++){
        char ch=s[i];
        ch=tolower(ch);
        if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='y')
        continue;
        cout<<"."<<ch;
        
    }
    return 0;
}
